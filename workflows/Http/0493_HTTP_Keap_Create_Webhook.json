      ],
      "parameters": {
        "color": 3,
        "width": 430,
        "height": 216,
        "content": "1. Create an Account a {{ $env.WEBHOOK_URL }}\n2. Create API Key ({{ $env.API_BASE_URL }}\n3. Update Node with your Credentials (Basic Auth)"
      },
      "typeVersion": 1,
      "notes": "This stickyNote node performs automated tasks as part of the workflow."
    },
    {
      "id": "4275e2a4-60a9-447e-8d64-f0073b9abe6b",
      "name": "Address Verification",
      "type": "n8n-nodes-base.httpRequest",
      "position": [
        1060,
