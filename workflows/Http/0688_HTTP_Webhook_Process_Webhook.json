        "jsonBody": "={\n  \"replyToken\": \"{{ $('Receive a Line Webhook').item.json.body.events[0].replyToken }}\",\n  \"messages\": [\n    {\n      \"type\": \"image\",\n      \"originalContentUrl\": \"{{ $json.url }}\",\n      \"previewImageUrl\": \"{{ $json.url }}\"\n    }\n  ]\n}",
