        }
      },
      "credentials": {
        "highLevelApi": {
          "id": "qJqOS89WQuqj4wXh",
          "name": "Test"
        }
      },
      "typeVersion": 1,
      "notes": "This highLevel node performs automated tasks as part of the workflow."
    },
    {
      "id": "9f896b41-eeb9-4cde-9fc8-e1ba000a2b61",
      "name": "Switch",
      "type": "n8n-nodes-base.switch",
      "position": [
        1280,
        280
      ],
      "parameters": {
        "rules": {
          "rules": [
            {
              "value2": "=deliverable",
              "outputKey": "YOUR_CREDENTIAL_HERE"
            },
            {
              "value2": "deliverable",
              "operation": "notEqual",
              "outputKey": "YOUR_CREDENTIAL_HERE"
            }
          ]
        },
        "value1": "={{ $json.deliverability }}",
        "dataType": "string"
      },
      "typeVersion": 2,
      "notes": "This switch node performs automated tasks as part of the workflow."
    }
