        "replyMarkup": "inlineKeyboard",
        "inlineKeyboard": {
          "rows": [
            {
              "row": {
                "buttons": [
                  {
                    "text": "={{ $('Github Gist').item.json.files[\"email.html\"].filename }}",
                    "additionalFields": {
                      "url": "{{ $env.BASE_URL }}"
                    }
                  }
                ]
              }
            }
          ]
        },
        "additionalFields": {
          "parse_mode": "HTML",
          "appendAttribution": true,
          "disable_web_page_preview": true
