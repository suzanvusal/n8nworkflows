      "parameters": {
        "path": "1fea1f5f-81c0-48ad-ae13-41e0f8e474ed",
        "options": {},
        "httpMethod": "POST"
      },
      "typeVersion": 1.1,
      "notes": "This webhook node performs automated tasks as part of the workflow."
    }
