        "select": "channel",
        "channelId": {
          "__rl": true,
          "mode": "name",
          "value": "#dk-test"
        },
        "otherOptions": {
          "mrkdwn": true
        }
      },
      "typeVersion": 2.2,
      "notes": "This slack node performs automated tasks as part of the workflow."
